{
  "name": "@ianhomer/gatsby-plugin-things",
  "description": "Gatsby plugin which renders markdown things.",
  "version": "0.0.4",
  "author": "Ian Homer",
  "bugs": {
    "url": "https://github.com/ianhomer/gatsby-plugin-things/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "node-fetch": "^3.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-source",
    "gatsby-plugin",
    "cat facts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianhomer/gatsby-plugin-things.git"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./",
    "jest": "jest",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir .",
    "test": "jest",
    "update": "npx npm-check-updates -u && npm install --package-lock-only --no-audit"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/compat-data": "^7.15.0",
    "@babel/core": "^7.15.8",
    "acorn": ">=8.5.0",
    "babel-preset-gatsby-package": "^1.14.0",
    "cross-env": "^7.0.3",
    "jest": "^27.3.0",
    "lodash": "^4.17.21"
  },
  "license": "MIT",
  "peerDependencies": {
    "gatsby": ">3.14.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
