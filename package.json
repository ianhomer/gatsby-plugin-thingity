{
  "name": "gatsby-plugin-thingity",
  "description": "Gatsby plugin which renders markdown things",
  "version": "0.0.1",
  "author": "Ian Homer",
  "bugs": {
    "url": "https://github.com/ianhomer/thingity/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "gatsby": "^2.18.6",
    "gatsby-core-utils": "^1.0.22",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-offline": "^3.0.24",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.4",
    "gatsby-source-filesystem": "^2.1.39",
    "gatsby-transformer-sharp": "^2.3.6",
    "gatsby-transformer-remark": "^2.6.24",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "sanitize-html": "^1.20.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "^16.9.0",
    "babel-preset-gatsby-package": "^0.2.12",
    "cross-env": "^6.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "prettier": "1.18.2",
    "typescript": "3.6.3"
  },
  "homepage": "https://github.com/ianhomer/thingity",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "markdown"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.18.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianhomer/thingity.git"
  },
  "scripts": {
    "build": "npm run build:src",
    "build:src": "babel src --out-dir . --ignore **/__tests__",
    "lint": "eslint --ext .tsx ./src",
    "lint:fix": "eslint --ext .tsx --fix ./src",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "npm run build:src -- --watch"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
